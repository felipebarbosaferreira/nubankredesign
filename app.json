{
  "expo": {
    "name": "Nubank Redesing",
    "slug": "nubankredesign",
    "description": "Projeto Nubank Redesign com Assistente Virtual feito em React Native",
    "version": "1.0.0",
    "orientation": "portrait",
    "githubUrl": "https://github.com/felipebarbosaferreira/nubankredesign.git",
    "icon": "./src/assets/nu_symbol_offwhite.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#82269e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.github.felipebarbosaferreira.nubankredesign"
    },
    "web": {
      "favicon": "./src/assets/nu_symbol_offwhite.png"
    },
    "android": {
      "package": "com.github.felipebarbosaferreira.nubankredesign",
      "icon": "./src/assets/nu_symbol_offwhite.png"
    }
  }
}
